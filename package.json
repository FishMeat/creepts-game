{
    "name": "creepts",
    "version": "0.0.1",
    "description": "Tower defense game for Cartesi",
    "devDependencies": {
        "@babel/core": "^7.6.2",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/plugin-proposal-export-default-from": "^7.5.2",
        "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
        "@babel/plugin-transform-modules-commonjs": "^7.6.0",
        "@babel/preset-env": "^7.6.2",
        "@babel/preset-typescript": "^7.6.0",
        "@beam-australia/react-env": "^2.1.2",
        "@types/hookrouter": "^2.2.1",
        "@types/jest": "^24.0.18",
        "@types/node": "^12.7.4",
        "@types/react": "^16.9.2",
        "@types/react-dom": "^16.9.0",
        "@types/semantic-ui": "^2.2.7",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.0.4",
        "css-loader": "3.2.0",
        "expose-loader": "0.7.5",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^24.9.0",
        "style-loader": "1.0.0",
        "ts-loader": "6.0.4",
        "typescript": "3.6.2",
        "webpack": "4.35.3",
        "webpack-bundle-analyzer": "^3.5.2",
        "webpack-bundle-tracker": "^0.4.3",
        "webpack-cli": "3.3.5",
        "webpack-dev-server": "3.7.2",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "@types/howler": "^2.1.1",
        "@types/timsort": "^0.3.0",
        "commander": "^4.0.1",
        "hookrouter": "^1.2.3",
        "howler": "^2.1.2",
        "moment": "^2.24.0",
        "phaser": "3.19.0",
        "query-string": "^6.8.3",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "semantic-ui-css": "^2.4.1",
        "semantic-ui-react": "^0.88.1",
        "timsort": "^0.3.0",
        "typed-emitter": "^0.1.0"
    },
    "scripts": {
        "dev": "webpack-dev-server --env.GAME_ONLY=true --mode development",
        "dev:cartesi": "mkdir -p dist/ && react-env --env .env --dest ./dist && webpack-dev-server --mode development",
        "test": "jest verifier.test",
        "build": "webpack --mode production && react-env --env .env --dest ./dist",
        "build:game": "webpack --env.GAME_ONLY=true --mode production && react-env --env .env --dest ./dist",
        "build:app": "webpack --config-name app --mode production && react-env --env .env --dest ./dist",
        "build:cmdline": "webpack --config-name cmdline --mode production",
        "verifier": "webpack --config-name cmdline --mode production && node dist/node-verifier-bundle.js",
        "levels": "webpack --config-name cmdline --mode production && node dist/levels-bundle.js"
    }
}
